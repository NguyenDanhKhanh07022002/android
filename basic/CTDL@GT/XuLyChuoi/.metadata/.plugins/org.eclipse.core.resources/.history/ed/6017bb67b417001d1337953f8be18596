
public class vidu {
	public static void main(String[] args) {
		String s1 = "TiVi"; 
		String s2 = ".vn"; 
		
		// có thể nối chuỗi 
		String s3 = s1 + s2 ; 
		//hàm nối chuỗi 
		String s4 = s1.concat(s2); // dùng hàm nối chuỗi sẽ không làm thày đổi s1 và s2
		System.out.println(s3);
		System.out.println(s4);
		
	}
}
